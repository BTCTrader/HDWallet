<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1;net5.0</TargetFrameworks>
  
    <Title>HDWallet.BIP32.Ed25519</Title>
    <Description>Generic HD wallet</Description>
    <Version>0.2.0</Version>

    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/BTCTrader/HDWallet</RepositoryUrl> 
    <PackageProjectUrl>https://github.com/BTCTrader/HDWallet</PackageProjectUrl>
    
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>HDWallet;Ed25519;BIP32;Blockchain</PackageTags>
    <PackageId>BTCTrader.HDWallet.BIP32.Ed25519</PackageId>
    
    <Authors>BTCTrader</Authors>
    <Company>BTCTrader</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NBitcoin" Version="7.0.6" />
  </ItemGroup>

  <PropertyGroup>
    <NoWarn>CS0809</NoWarn>
  </PropertyGroup>

</Project>
